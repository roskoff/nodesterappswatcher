{
  "manifest_version": 2,
  "name": "Nodester Application Watcher",
  "version": "0.6",
  "description": "This extension checks the status of your Nodester applications",
  "browser_action": {
    "default_icon": "/img/naw-icon128.png",
    "popup": "popup.html"
  },
  "icons": {
    "128": "/img/naw-icon128.png",
    "48": "/img/naw-icon48.png",
    "16": "/img/naw-icon16.png"
  },
  "permissions": [
    "http://*.nodester.com/"
  ]
}
